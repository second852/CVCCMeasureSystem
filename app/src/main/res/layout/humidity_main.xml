<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f5f5f5"
    >

    <RelativeLayout
        android:layout_marginTop="10dp"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@drawable/line_model"
        >
        <com.beardedhen.androidbootstrap.AwesomeTextView
            android:layout_centerHorizontal="true"
            android:padding="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/sample1"
            android:text="Sample 1"
            android:textSize="18sp"
            app:bootstrapBrand="primary" />
        <ImageView
            android:layout_below="@+id/sample1"
            android:padding="5dp"
            android:id="@+id/sensor1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/lighte"
            />
        <LinearLayout
            android:padding="5dp"
            android:layout_toEndOf="@+id/sensor1"
            android:layout_below="@+id/sample1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <com.beardedhen.androidbootstrap.AwesomeTextView
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="Now V (mV)"
                    android:textSize="15sp"
                    app:bootstrapBrand="regular" />
                <TextView
                    android:background="@drawable/corners_model"
                    android:id="@+id/nowVoltage1"
                    android:text="0"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="3dp"
                    android:gravity="center"
                    android:textSize="15sp"
                    android:hint="7"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHint="#AAAAAA" />

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_gravity="left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <com.beardedhen.androidbootstrap.AwesomeTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="Limit V(mV)"
                    android:textSize="15sp"
                    app:bootstrapBrand="warning" />
                <com.beardedhen.androidbootstrap.BootstrapEditText
                    android:id="@+id/limit1"
                    android:text="4"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="3dp"
                    android:gravity="center"
                    android:hint="7"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHint="#AAAAAA"
                    app:bootstrapBrand="primary"
                    app:bootstrapSize="sm"
                    app:roundedCorners="true" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

