<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    >
    <TextView
        android:id="@+id/senMessage"
        android:text="@string/measure_stop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        />
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/sample1I"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/lighte"
                />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample1N"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="S 1"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample1R"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="S 1"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample1S"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="---mv/pH"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/sample2I"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/lighte"
                />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample2N"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="S 2"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/sample3I"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/lighte"
                />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample3N"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="S 3"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/sample4I"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/lighte"
                />
            <com.beardedhen.androidbootstrap.AwesomeTextView
                android:id="@+id/sample4N"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="S 4"
                android:textSize="18sp"
                app:bootstrapBrand="primary" />
        </LinearLayout>
    </LinearLayout>
    <ListView
        android:id="@+id/listData"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f5f5f5"
        android:dividerHeight="0dp"
        >
    </ListView>
</LinearLayout>
